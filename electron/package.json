{
  "name": "pine",
  "version": "1.0.0",
  "description": "Protein Interaction Network Extractor",
  "main": "main.js",
  "scripts": {
    "start": "npm run-script sass && electron .",
    "dev": "npm run-script sass && set NODE_ENV=dev&& electron .",
    "devj": "npm run-script sass && set NODE_ENV=devj&& electron .",
    "sass": "sass app/index.scss app/css/index.css"
  },
  "keywords": [
    "protein",
    "proteomics",
    "cytoscape"
  ],
  "author": "Cedars-Sinai",
  "license": "Apache License 2.0",
  "devDependencies": {
    "electron": "^6.0.7",
    "electron-builder": "^21.2.0"
  },
  "dependencies": {
    "vue": "^2.6.10"
  },
  "build": {
    "productName": "Pine",
    "extraResources": [
      {
        "from": "../dist/changes_to_pine_final",
        "to": "extra-resources/pine_2"
      }
    ],
    "win": {
      "target": "nsis",
      "icon": "app/assets/pine-tree.png"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}
