{
  "name": "pine",
  "version": "1.0.0",
  "description": "Protein Interaction Network Extractor",
  "main": "main.js",
  "scripts": {
    "start": "npm run-script sass && electron .",
    "dev": "npm run-script sass && set NODE_ENV=dev&& electron .",
    "sass": "sass app/index.scss app/css/index.css"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "Cedars-Sinai",
  "license": "Proprietary",
  "devDependencies": {
    "electron": "^6.0.7",
    "electron-packager": "^14.0.6",
    "electron-builder": "^21.2.0"
  },
  "dependencies": {
    "vue": "^2.6.10"
  },
  "build": {
    "productName": "Pine",
    "extraResources": [
      {"from": "../dist/changes_to_pine_final.exe", "to": "extra-resources/pine_2.exe"}
    ],
    "win": {
      "target": "portable",
      "icon": "app/assets/pine-tree.png"
    }
  }
}
