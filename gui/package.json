{
  "name": "pine",
  "version": "2.0.1",
  "description": "Protein Interaction Network Extractor",
  "main": "main.js",
  "scripts": {
    "start": "npm run-script sass && electron .",
    "dev": "npm run-script sass && set NODE_ENV=dev&& electron .",
    "sass": "sass app/index.scss app/css/index.css"
  },
  "keywords": [
    "protein",
    "proteomics",
    "cytoscape"
  ],
  "author": "Cedars-Sinai",
  "license": "Apache License 2.0",
  "devDependencies": {
    "minimist": ">=1.2.5",
    "electron": "^7.2.4",
    "lodash": "^4.17.19",
    "sass": "^1.26.3",
    "electron-builder": "^21.2.0"
  },
  "dependencies": {
    "vue": "^2.6.11"
  },
  "build": {
    "appId": "org.cshs.pine",
    "productName": "Pine",
    "extraResources": [
      {
        "from": "../dist/pine",
        "to": "extra-resources/pine"
      }
    ],
    "win": {
      "target": "nsis",
      "icon": "app/assets/pine-tree.png"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "artifactName": "pine-setup.exe"
    }
  }
}
